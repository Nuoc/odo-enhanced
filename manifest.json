{
	"manifest_version": 2,
	"name": "Odo Enhanced",
	"description": "Flat Theme, Favicons, Autofill Tickets, QWiki Integration, Improved Jira Integration, Visible Snippets",
	"version": "1.6.1.0",


	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/",
		"http://odo.corp.qualtrics.com/*",
		"http://qwiki.dev.qualtrics.com/*",
		"https://zachs-webservices.herokuapp.com/*",
		"http://mcdonnellteach.com/*",
		"storage",
		"tabs"
	],
	"web_accessible_resources": ["resources/options.html", "css/starWars.css", "css/squawkPosts.css", "css/greyAlerts.css"],
	"icons": {
		"128": "img/icon.png"
	},
	"options_page": "resources/options.html",
	"content_scripts": [
		{
			"matches": ["http://odo.corp.qualtrics.com/*"],
			"js": ["resources/jquery-1.11.3.min.js", "resources/jquery.dataTables.min.js", "js/quni.js", "resources/tinyeditor/tinyeditor.js", "js/starWars.js", "resources/firebase.js"],
			"css": ["resources/jquery.dataTables.css", "css/odo-enhanced.css", "resources/tinyeditor/style.css"],
			"run_at": "document_end"
	}
	]
}
