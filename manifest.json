{
    "manifest_version": 2,

    "name": "Odo Enhanced",
    "description": "Flat Theme, Favicons, Autofill Tickets, QWiki Integration, Improved Jira Integration",
    "version": "1.3",
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "http://odo.corp.qualtrics.com/*",
        "http://qwiki.dev.qualtrics.com/*",
        "https://zachs-webservices.herokuapp.com/*"
    ],
    "icons": { "128": "icon.png"},
    "content_scripts": [
    {
    "matches": ["http://odo.corp.qualtrics.com/*"],
    "js": ["resources/jquery-1.11.3.min.js","resources/jquery.dataTables.min.js","quni.js"],
    "css": ["resources/jquery.dataTables.css","odo-enhanced.css"]
    }
    ]
}